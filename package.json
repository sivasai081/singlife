    "@tippyjs/react": "^4.2.5",
    "@types/recharts": "^1.8.24",
    "axios": "^1.1.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "date-fns": "^2.25.0",
    "dompurify": "^3.1.6",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "formik": "^2.2.9",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-resolve": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lottie-web": "^5.7.14",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-datepicker": "^4.10.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-idle-timer": "^5.7.2",
    "react-intersection-observer": "^9.4.3",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.4.2",
    "react-transition-group": "^4.4.2",
    "recharts": "^2.3.2",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "semver": "^7.3.5",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "swiper": "^9.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:ci": "GENERATE_SOURCEMAP=true node scripts/build.js",
    "build:prd:ci": "GENERATE_SOURCEMAP=false node scripts/build.js",
    "test": "node scripts/test.js",
    "test:unit": "node scripts/test.js --reporters=default --reporters=jest-junit --coverage",
    "test:only-changed": "node scripts/test.js --findRelatedTests --passWithNoTests",
    "prepare": "husky install .husky",
    "lint-staged": "lint-staged",
    "lint:type": "tsc",
    "eslint": "eslint --max-warnings=0 src/**/*.{ts,tsx,js}",
    "stylelint": "stylelint --max-warnings=0 \"src/**/*.tsx\"",
    "stylelint:fix": "stylelint --max-warnings=0 --fix \"src/**/*.tsx\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@svgr/webpack": "^6.2.1",
    "@tanstack/eslint-plugin-query": "^4.24.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.55",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-transition-group": "^4.4.3",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.0",
    "husky": "^7.0.4",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.0.1",
    "postcss-styled-syntax": "^0.3.3",
    "prettier": "^2.3.1",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack",
    "json5": "^2.2.2",
    "glob-parent": "^6.0.1",
    "debug": "^4.3.4",
    "tough-cookie": "^4.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "npm run test:only-changed"
    ]
  },
  "jest": {
    "testTimeout": 15000,
    "bail": 1,
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/husky/**",
      "!**/react-app-env.d.ts",
      "!**/index.tsx",
      "!**/reportWebVitals.tsx",
      "!**/src/config/**",
      "!**/src/@types/**"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!swiper|ssr-window|dom7|tippy.js|react-datepicker|@okta|recharts)"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
